{  
   "_version":"0.0.1",
   "sap.app":{  
      "name":"com.test",
      "version":"0.0.1",
      "description":"Sample application to understand component.js/json",
      "dependencies":{  

      }
   },
   "sap.ui5":{  
      "_version":"1.1.0",
      "rootView":"com.test.View.App",
      "dependencies":{  
         "minUI5Version":"1.40",
         "libs":{  
            "sap.m":{  

            }
         }
      },
      "resources":{  
         "css":[  

         ]
      },
      "routing":{  
         "config":{  
            "routerClass":"sap.m.routing.Router",
            "viewType":"XML",
            "viewPath":"com.test.View",
            "controlId":"app",
            "controlAggregation":"pages"
         },
         "routes":[  
            {  
               "pattern":"",
               "name":"overview",
               "target":"overview"
            },
            {  
               "pattern":"StudentDetails",
               "name":"StudentDetails",
               "target":"StudentDetails"
            },
            {  
               "pattern":"StudentSessions/:IV:",
               "name":"StudentSessions",
               "target":"StudentSessions"
            },
            {  
               "pattern":"Questions/:obj1:/:obj2:",
               "name":"Questions",
               "target":"Questions"
            },
            {  
               "pattern":"DashBoard",
               "name":"DashBoard",
               "target":"DashBoard"
            },
            {  
               "pattern":"Games",
               "name":"Games",
               "target":"Games"
            },
            {  
               "pattern":"VideosHTML",
               "name":"VideosHTML",
               "target":"VideosHTML"
            },
            {  
               "pattern":"GalleryPhotos",
               "name":"GalleryPhotos",
               "target":"GalleryPhotos"
            },
            {  
               "pattern":"GallerySongs",
               "name":"GallerySongs",
               "target":"GallerySongs"
            },
            {  
               "pattern":"SiddalingaAppaji",
               "name":"SiddalingaAppaji",
               "target":"SiddalingaAppaji"
            },
            {  
               "pattern":"MurugharajAppaji",
               "name":"MurugharajAppaji",
               "target":"MurugharajAppaji"
            },
            {  
               "pattern":"YallalingaAppaji",
               "name":"YallalingaAppaji",
               "target":"YallalingaAppaji"
            },
            {  
               "pattern":"SiddaramShivayogi",
               "name":"SiddaramShivayogi",
               "target":"SiddaramShivayogi"
            }
                        
         ],
         "targets":{  
            "overview":{  
               "viewName":"LogIn"
            },
            "StudentDetails":{  
               "viewName":"StudentDetails"
            },
            "StudentSessions":{  
               "viewName":"StudentSessions"
            },
            "Questions":{  
               "viewName":"QandA"
            },
            "DashBoard":{  
               "viewName":"DashBoard",
               "transition":"show"
            },
            "Games":{  
               "viewName":"Games",
               "transition":"show"
            },
           
            "GalleryPhotos":{  
               "viewName":"Gallery.GalleryPhotos",
               "transition":"show",
               "viewType": "XML"
            },
             "GallerySongs":{  
               "viewName":"Gallery.GallerySongs",
               "transition":"show",
               "viewType": "HTML"
            },
            "VideosHTML":{  
               "viewName":"Gallery.VideosHTML",
               "transition":"show",
               "viewType": "HTML"
            },
            "SiddalingaAppaji":{  
               "viewName":"DivyaShakti.SiddalingaAppaji",
               "transition":"show",
               "viewType": "HTML"
            },
            "MurugharajAppaji":{  
               "viewName":"DivyaShakti.MurugharajAppaji",
               "transition":"show",
               "viewType": "HTML"
            },
            "SiddaramShivayogi":{  
               "viewName":"DivyaShakti.SiddaramShivayogi",
               "transition":"show",
               "viewType": "HTML"
            },
            "YallalingaAppaji":{  
               "viewName":"DivyaShakti.YallalingaAppaji",
               "transition":"show",
               "viewType": "HTML"
            }
            
         }
      }
