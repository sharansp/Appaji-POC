	<Page id="mainContent" title="{i18n>title}">
				<content>
					<Panel expandable="true" expanded="true" width="100%" height="auto">
						<headerToolbar>
							<Toolbar height="3em">
								<Title text="Testing Graph Component"/>
								<ToolbarSpacer/>
								<Label text="Switch Measure" labelFor="toggleButton"/>
								<SegmentedButton id="__button1">
									<items>
										<SegmentedButtonItem text="Weight" id="btnWeight" press="onSwitchMeasure"/>
										<SegmentedButtonItem text="Betweenness" id="btnBetweenness" press="onSwitchMeasure"/>
										<SegmentedButtonItem text="Closeness" id="btnCloseness" press="onSwitchMeasure"/>
										<SegmentedButtonItem text="PageRank" id="btnRank" press="onSwitchMeasure"/>
									</items>
								</SegmentedButton>
								<content/>
							</Toolbar>
						</headerToolbar>
						<content>
							<FlexBox width="100%" id="idFlexBoxFL" height="100%" alignItems="Center" alignContent="Center" justifyContent="Center">
								<!-- <fl:InteractiveNetwork id="graph" interactive="false" graph="{path: 'graphModel>/graph'}" nodeSelected="onNodeSelected"
									measure="{/graphMeasure}"/> -->
							</FlexBox>
						</content>
					</Panel>
				</content>
			</Page>


		
